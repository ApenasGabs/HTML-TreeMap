{
  "name": "html-tree-view",
  "version": "0.1.0",
  "description": "A React component to visualize HTML structures as a tree",
  "type": "module",
  "main": "./dist/html-tree-view.umd.js",
  "module": "./dist/html-tree-view.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/html-tree-view.es.js",
      "require": "./dist/html-tree-view.umd.js",
      "default": "./dist/html-tree-view.es.js"
    }
  },
  "keywords": [
    "react",
    "html",
    "tree",
    "visualization",
    "parser"
  ],
  "author": "Gabriel Rodrigues",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/apenasgabs/html-tree-view"
  },
  "private": false,
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "prepare": "npm run build"
  },
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@types/node": "^22.15.2",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.22.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.3.1"
  }
}
